SECTIONS
{
    .fixedCode : > `utils.toHex(this.fixedCodeAddr)`
    {
        *(.text)
        *(.const)
    }


    GROUP : run = `utils.toHex(this.fixedDataAddr)`
    {
        .fixedData:modobj__C:
        {
%for each (var sn in this.$capsule._romAsm.sectNamesC) {
            . = align(4);
            *(.modobj__C:`sn`)
%}    
        }
        .fixedData:modobj__V:
        {
%for each (var sn in this.$capsule._romAsm.sectNamesV) {
            *(.modobj__V:`sn`)
%}    
        }
        .fixedData:far: { *(.far) } 
    }
}

