 *  ======== C64.tci ========
 *
 */

function C64(name, parent)
{
    /*
     *  'this' refers to the cpu object in the object model
     */
    this.attrs = {};
    this.attrs.cpuCore = "6400";
    this.attrs.minDataUnitSize = 1;
    this.attrs.dataWordSize = 4;
    this.attrs.minProgUnitSize = 1;
}
